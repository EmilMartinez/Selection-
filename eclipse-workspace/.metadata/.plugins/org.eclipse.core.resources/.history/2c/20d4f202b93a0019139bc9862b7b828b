
package ReviewofProject23456;

public class Father {

	public String Height(String Height) {
	String total = Height;
		System.out.println("I am 6ft tall.");
	return total;
		}
	
	public int Hair(int length) {
	int total = length*2;
	System.out.println("My hair color is Black.");
	return total;
}
	
	public String Hands(String Color) {
	String total = Color;
	System.out.println("My hands are larger than normal.");
	return total;
	}
	
	public String Eyes(String eyeColor) {
	String total = eyeColor; 
	System.out.println("My eyes are blue");	
	return total;
		
}

	}
