package ReviewofProject23456;

public class ifelse {
	public static void main(String[] args) {

		int num1 = 300;
		int num2 = 450;
		
		if (num1 > num2) {
		System.out.println(num1+ " is bigger then" +num2);
		}else {
		System.out.println(num2 + " is bigger then " +num2);
		}

		
	}

	
	
	
	
	
	
	
	
	
	
	
}
