
public class MyDreamCar {

public String carName;
public float carWeight;
public int carYear;
public double carCost;
  
public MyDreamCar (String carNa) {
	this.carName = carna;
	System.out.println(carNa);
}

public MyDreamCar (float carWei) {
	this.carWeight = carWei;
	System.out.println(carWei);
}

public MyDreamCar (int carYe) {
	this.carYear = carYe;
	System.out.println(carYe);
}

public MyDreamCar (double carCo) {
	this.carCost = carCo;
	System.out.println(carCo);
}


}
