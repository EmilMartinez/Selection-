package ReviewofProject23456;

public class Operatoruse {

	public static void main(String[] args) {
	int num1 = 300; 
	int num2 = 250; 
	int num3 = 450; 
	if (num1 >num2); {
	System.out.println(num1 + "is greater" +num2);
	} if (num3 > num2); 
	System.out.println(num3+ "is greater" +num2);
	
	}
}


