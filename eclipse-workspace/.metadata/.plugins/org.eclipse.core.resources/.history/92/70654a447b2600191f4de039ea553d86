
public class workit {

	  public int multiplication (int e, int c) {
		  int h = e*c;
		  System.out.println(" the total is:" +h); 
		  return e;
		  }
	  
	  public int addition (int w, int s) {
		  int t = w+s; 
		  System.out.println(" the total is:" +t);
		  return t; 
	      }
	  
	  public int divison (int p, int y) { 
		  int j = p/y; 
		  System.out.println(" the total is:" +j);
		  return j;
	  }
	  
	  public int subtraction (int o, int b) {
		  int g = o-b;
		  System.out.println(" the total is:" +g); 
		  return g;
	  }
	  
	  public int exponents (int u) {
		  int v = u^2;
		  System.out.println(" the total is:" +v);
		  return v;
	  }
	  
	 	  
}
