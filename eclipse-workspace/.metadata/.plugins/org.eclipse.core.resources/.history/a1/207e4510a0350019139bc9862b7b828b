package ReviewofProject23456;

import java.util.Scanner;

public class ScannerPractice {

	public static void main(String[] args) {
		System.out.println("How many kids are there in School?");
		Scanner sc = new Scanner(System.in);
		int kids = sc.nextInt();
		int total = kids;
		System.out.println("The are:"+total);
		sc.close();
		
		
		
}
}
