public class vehicles 
{

	//method 1 response method
	public int addition(int v, int g)
	{
		int e = v + g; 
		System.out.println("the total is: "+e); 
		return e;
	}
	
	public int multiplicating(int a, int t) {
		int w = a*t; 
		System.out.println("the total is: "+w);
		return w; 
		}
	
	
	public int divison(int s, int e) { 
	int t = s/e; 
	    System.out.println("the total is: "+t);
	    return t; 
	 }
    
	
}
