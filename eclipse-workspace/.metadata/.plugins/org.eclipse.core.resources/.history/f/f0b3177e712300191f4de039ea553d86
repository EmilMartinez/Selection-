package classnmethodnobjects;

public class Calculation {
	//method1 non return method
	   public void methodname () {
		   int a = 2;
		   int b =  3;
		   int c =  a+b;
	   System.out.println ("the total is: " +c); 
	   }
	   
	  //method 1 return method 
		public int addition(int a, int b) {
			int c = a+b; 
			System.out.println("the total is: "+c);
			return c; 
		}

}
